{
    "name": "string-lookup-manager",
    "version": "1.1.0",
    "description": "An easy way to collect regular expressions search results",
    "homepage": "https://github.com/keindev/string-lookup-manager#readme",
    "license": "MIT",
    "author": {
        "name": "Daniil Ryazanov",
        "email": "kein@tagproject.ru"
    },
    "keywords": [
        "substitutions",
        "string"
    ],
    "main": "lib/manager.js",
    "module": "lib/manager.js",
    "types": "lib/manager.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/keindev/string-lookup-manager.git"
    },
    "bugs": {
        "url": "https://github.com/keindev/string-lookup-manager/issues"
    },
    "scripts": {
        "clean": "rimraf lib coverage",
        "lint": "eslint src/**/*.ts --fix",
        "spellcheck": "cspell -c .vscode/cspell.json --no-summary \"*.*\" \"**/*.ts\" \"**/*.js\"",
        "test": "jest",
        "docs": "typedoc",
        "changelog": "changelog generate",
        "build:ts": "tsc",
        "build": "npm-run-all clean spellcheck lint test build:* docs"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/faker": "^4.1.9",
        "@types/jest": "^24.9.0",
        "@types/node": "^13.1.8",
        "@typescript-eslint/eslint-plugin": "^2.3.1",
        "@typescript-eslint/parser": "^2.17.0",
        "changelog-guru": "^1.0.0",
        "cspell": "^4.0.44",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jest": "^23.6.0",
        "faker": "^4.1.0",
        "husky": "^4.0.10",
        "jest": "^24.9.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.18.2",
        "ts-jest": "^24.3.0",
        "typedoc": "^0.16.7",
        "typedoc-plugin-markdown": "^2.2.16",
        "typescript": "^3.7.5"
    }
}
