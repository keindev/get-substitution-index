{
  "name": "string-lookup-manager",
  "version": "1.1.1",
  "description": "Easy way to collect regular expressions search results",
  "homepage": "https://github.com/keindev/string-lookup-manager#readme",
  "license": "MIT",
  "author": {
    "name": "Daniil Ryazanov",
    "email": "kein@tagproject.ru"
  },
  "keywords": [
    "substitutions",
    "string"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keindev/string-lookup-manager.git"
  },
  "bugs": {
    "url": "https://github.com/keindev/string-lookup-manager/issues"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:ts": "rimraf lib && tsc --extendedDiagnostics",
    "check": "npm-run-all check:*",
    "check:lint": "rimraf coverage && eslint \"src/**\" --ext .ts",
    "check:spell": "cspell -c .vscode/cspell.json --no-summary \"README.md\" \"src/**/*.ts\"",
    "generate": "npm-run-all generate:*",
    "generate:changelog": "changelog generate --bump",
    "generate:docs": "rimraf docs/api && typedoc",
    "generate:ghinfo": "ghinfo generate -d media -t utils",
    "prepare": "npm-run-all prepare:*",
    "prepare:config": "ts-package-shared-config",
    "prepare:husky": "husky install",
    "release": "npm-run-all prepare check test build generate",
    "test": "npm-run-all test:*",
    "test:jest": "jest"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.0",
    "@tagproject/ts-package-shared-config": "^1.5.8",
    "@types/faker": "^5.5.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^26.6.3",
    "changelog-guru": "^2.3.0",
    "cspell": "^5.3.12",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-promise": "^5.1.0",
    "faker": "^5.5.3",
    "ghinfo": "^1.0.8",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5",
    "typedoc": "^0.20.36",
    "typedoc-plugin-markdown": "^3.7.2",
    "typescript": "^4.2.4"
  }
}
